Name: libSBML
Description: A library for reading/writing/manipulating SBML
URL: http://sbml.org/Software/libSBML
Version: 5.18.0

prefix=/usr/local
libdir=/usr/local/lib
includedir=/usr/local/include

Libs: -L${libdir} -lsbml
Libs.private:        /usr/lib/x86_64-linux-gnu/libxml2.so /usr/lib/x86_64-linux-gnu/libbz2.so /usr/lib/x86_64-linux-gnu/libz.so -lstdc++ -lm
Cflags: -I${includedir}
Requires.private: libxml-2.0 >= 2.6.22
